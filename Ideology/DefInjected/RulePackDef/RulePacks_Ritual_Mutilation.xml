<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN:
    <li>r_ritualName->[ritNoun] of [keyDeity]</li>
    <li>r_ritualName->[ritNoun] of [chosenTheme]</li>
    <li>r_ritualName->[ritNoun] of [memeConcept]</li>
    <li>r_ritualName->[memeAdjective] [ritNoun]</li>
    <li>ritNoun->mutilation</li>
    <li>ritNoun->cutting</li>
    <li>ritNoun->slicing</li>
    <li>ritNoun->peeling</li>
    <li>ritNoun->bladerip</li>
    <li>ritNoun->ripping</li>
    <li>ritNoun->mangling</li>
    <li>ritNoun->fleshcarving</li>
    <li>ritNoun->gashing</li>
    <li>ritNoun->rend</li>
  -->
  <NamerRitualMutilation.rulePack.rulesStrings>
    <li>r_ritualName->[ritNoun] von [keyDeity]</li>
    <li>r_ritualName->{replace: [keyDeity]'s; "Gott's"-"Gottes"} [ritNoun]</li>
    <li>r_ritualName->[ritNoun] [chosenTheme]</li>
    <li>r_ritualName->[ritNoun] [memeConceptGenDef]</li>
    <li>r_ritualName->[memeAdjective]e [ritNoun]</li>
    <li>r_ritualName->[memeComPrefix][ritNounComSuffix]</li>
    <li>r_ritualName->[memeHyphenPrefix]-[ritNoun]</li>
    <li>ritNoun->Verstümmelung</li>
    <li>ritNoun->Beschneidung</li>
    <li>ritNoun->Hautritzung</li>
    <li>ritNoun->Narbenritzung</li>
    <li>ritNoun->Skarifizierung</li>
    <li>ritNounComSuffix->verstümmelung</li>
    <li>ritNounComSuffix->beschneidung</li>
    <li>ritNounComSuffix->ritzung</li>
    <li>ritNounComSuffix->skarifizierung</li>
    <!-- gender-specific -->
    <li>memeConceptGenDef->des [memeConceptMasGen]</li>
    <li>memeConceptGenDef->der [memeConceptFemGen]</li>
    <li>memeConceptGenDef->des [memeConceptNeuGen]</li>
  </NamerRitualMutilation.rulePack.rulesStrings>
  
</LanguageData>