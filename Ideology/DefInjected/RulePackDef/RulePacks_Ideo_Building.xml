  <!-- EN:
    <li>r_ideoBuildingName(p=2)->[memeAdjective] [buildingName]</li>
    <li>r_ideoBuildingName->[buildingName] of [chosenTheme]</li>
    <li>r_ideoBuildingName->[chosenAdjective] [buildingName]</li>
    <li>r_ideoBuildingName->[keyDeity]'s [buildingName]</li>
    <li>r_ideoBuildingName->[buildingName] of [keyDeity]</li>
    <li>r_ideoBuildingName->[buildingName] of [memeConcept]</li>
    <li>r_ideoBuildingName->[buildingName] of [chosenIdeoName]</li>
  -->
  <NamerIdeoBuilding.rulePack.rulesStrings>
    <li>r_ideoBuildingName(p=2)->{replace: [buildingNameGender]; "|M|"-"[memeAdjective]er "; "|F|"-"[memeAdjective]e "; "|N|"-"[memeAdjective]es "}</li>
    <li>r_ideoBuildingName(p=2)->[memeComPrefix][buildingNameComSuffix]</li>
    <li>r_ideoBuildingName->[buildingName] [chosenTheme]</li>
    <li>r_ideoBuildingName->{replace: [buildingNameGender]; "|M|"-"[chosenAdjective]er "; "|F|"-"[chosenAdjective]e "; "|N|"-"[chosenAdjective]es "}</li>
    <li>r_ideoBuildingName->{replace: [keyDeity]'s; "Gott's"-"Gottes"} [buildingName]</li>
    <li>r_ideoBuildingName->[buildingName] {replace: von [keyDeity]; "von Gott"-"Gottes"; "von Christus"-"Christi"; "von Jesus"-"Jesu"}</li>
    <li>r_ideoBuildingName->[buildingName] {replace: [memeConceptGenGender]; "|M|"-"des "; "|F|"-"der "; "|N|"-"des "}</li>
    <!-- <li>r_ideoBuildingName->[buildingName] {replace: des [chosenIdeoName]; "des |M|"-"des "; "des |F|"-"der "; "des |N|"-"des "}</li> -->
    <li>r_ideoBuildingName->[memeHyphenPrefix]-[buildingName]</li>
  </NamerIdeoBuilding.rulePack.rulesStrings>
  
