<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN:
    <li>festivalName(p=2)->[memeAdjective] [festival]</li>
    <li>festivalName->[festival] of [chosenTheme]</li>
    <li>festivalName->[chosenAdjective] [festival]</li>
    <li>festivalName->[keyDeity]'s [festival]</li>
    <li>festivalName->[festival] of [keyDeity]</li>
    <li>festivalName->[festival] of [memeConcept]</li>
    <li>festivalName->[festival] of [chosenIdeoName]</li>
    <li>festival->festival</li>
    <li>festival->celebration</li>
    <li>festival->party</li>
    <li>festival->advent</li>
    <li>festival->fair</li>
    <li>festival->jubilee</li>
    <li>festival->fiesta</li>
  -->
  <NamerFestivalGlobal.rulePack.rulesStrings>
    <li>festivalName(p=2)->[memeAdjective]e [festival]</li>
    <li>festivalName->[festival] [chosenTheme]</li>
    <li>festivalName->[chosenAdjective]e [festival]</li>
    <li>festivalName->[keyDeity]-[festival]</li>
    <li>festivalName->[festival] von [keyDeity]</li>
    <li>festivalName->[festival] [memeConceptGenDef]</li>
    <li>festivalName->[festival] {replace: des [chosenIdeoName]; "des |M|"-"des "; "des |F|"-"der "; "des |N|"-"des "}</li>
    <li>festival->Feier</li>
    <li>festival->Party</li>
    <li>festival->Fiesta</li>
    <li>festival->Festlichkeit</li>
    <li>festival->Veranstaltung</li>
    <!-- gender-specific -->
    <li>memeConceptGenDef->des [memeConceptMasGen]</li>
    <li>memeConceptGenDef->der [memeConceptFemGen]</li>
    <li>memeConceptGenDef->des [memeConceptNeuGen]</li>
  </NamerFestivalGlobal.rulePack.rulesStrings>
  
  <!-- EN:
    <li>r_ritualName->[festivalName]</li>
  -->
  <NamerRitualFestival.rulePack.rulesStrings>
    <li>r_ritualName->[festivalName]</li>
  </NamerRitualFestival.rulePack.rulesStrings>
  
</LanguageData>