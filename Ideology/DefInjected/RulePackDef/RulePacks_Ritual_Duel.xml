    <li>ritNounComSuffix->duell</li>
    <li>ritNounComSuffix->schlacht</li>
    <li>ritNounComSuffix->rauferei</li>
    <li>ritNounComSuffix->streit</li>
    <li>ritNounAdj->er Kampf</li>
    <li>ritNounAdj->es Duell</li>
    <li>ritNounAdj->e Schlacht</li>
    <li>ritNounAdj->e Rauferei</li>
    <li>ritNounAdj->er Wettkampf</li>
    <li>ritNounAdj->er Wettstreit</li>
