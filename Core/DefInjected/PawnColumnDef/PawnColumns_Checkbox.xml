  <FollowFieldwork.headerTip>Folge Bezugsperson bei Außeneinsätzen (Tiere jagen oder zähmen).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters. -->
  <Hunt.headerTip>Markiere Tiere, die von deinen Jägern gejagt werden sollen.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>In die Wildnis entlassen</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Schlachten</Slaughter.headerTip>
  
  <!-- EN: Sterilize -->
  <Sterile.headerTip>Sterilisieren</Sterile.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Markiere Wildtiere, die gezähmt werden sollen.</Tame.headerTip>
  
